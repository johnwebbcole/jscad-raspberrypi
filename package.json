{
  "name": "@jwc/jscad-raspberrypi",
  "version": "1.2.0",
  "description": "jscad parts library for RaspberryPi",
  "main": "jscad-raspberrypi.jscad",
  "scripts": {
    "test": "nyc ava -- --verbose",
    "build": "gulp all",
    "start": "gulp",
    "docs": "vuepress-jsdoc --source ./srcx --dist ./docs && npx vuepress build ./docs"
  },
  "keywords": [
    "jscad",
    "raspberrypi"
  ],
  "author": {
    "name": "John Cole",
    "email": "johnwebbcole@gmail.com",
    "url": "https://jwc.dev"
  },
  "homepage": "https://gitlab.com/johnwebbcole/jscad-raspberrypi",
  "bugs": "https://gitlab.com/johnwebbcole/jscad-raspberrypi/issues",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:johnwebbcole/jscad-raspberrypi.git"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.0",
    "@jwc/jscad-test-utils": "^1.0.6",
    "@vuepress/plugin-active-header-links": "^1.2.0",
    "@vuepress/plugin-google-analytics": "^1.2.0",
    "ava": "^2.3.0",
    "del": "^5.0.0",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-inject": "^5.0.4",
    "gulp-jscad-files": "^4.0.0",
    "gulp-jsdoc-to-markdown": "^1.2.1",
    "gulp-merge": "^0.1.1",
    "gulp-plumber": "^1.2.1",
    "gulp-terser": "^1.2.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^5.0.1",
    "looks-same": "^7.2.1",
    "nyc": "^14.1.1",
    "rollup": "^1.25.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "vuepress-jsdoc": "^1.8.1"
  },
  "dependencies": {
    "@jscad/csg": "^0.7.0",
    "@jscad/scad-api": "^0.5.1",
    "@jwc/jscad-utils": "^4.3.3"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/helpers/**/*.js"
    ],
    "sources": [
      "src/**/*.js"
    ],
    "require": [
      "esm",
      "@babel/register",
      "babel-polyfill",
      "./test/helpers/compat-env.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
